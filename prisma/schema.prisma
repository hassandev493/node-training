// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/node-training"
}

model users {
  id    Int        @id @default(autoincrement())
  name  String     
  email String
  type  user_types
}

enum user_types {
  user
  admin
  customer
}
